@{

}
@model Bookings;
<head>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <title>OurSpace</title>
    <meta content="" name="description">
    <meta content="" name="keywords">
    <link href="~/img/favicon.png" rel="icon">
    <link href="~/apple-touch-icon.png" rel="apple-touch-icon">
    <link href="~/css/main.css" rel="stylesheet">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Open+Sans:ital,wght@0,300;0,400;0,500;0,600;0,700;1,300;1,400;1,600;1,700&family=Amatic+SC:ital,wght@0,300;0,400;0,500;0,600;0,700;1,300;1,400;1,500;1,600;1,700&family=Inter:ital,wght@0,300;0,400;0,500;0,600;0,700;1,300;1,400;1,500;1,600;1,700&display=swap"
          rel="stylesheet">
    <link href="~/vendor/bootstrap/css/bootstrap-grid.css" rel="stylesheet">
    <link href="~/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
    <link href="~/vendor/aos/aos.css" rel="stylesheet">
    <link href="~/vendor/glightbox/css/glightbox.min.css" rel="stylesheet">
    <link href="~/vendor/swiper/swiper-bundle.min.css" rel="stylesheet">

</head>
<body>
    <h1> Are you sure you want to delete? </h1>
    <form asp-action="creativesDone" asp-controller="Dashboard" method="post">
        <div class="form-group">
            <label asp-for="BId">Id</label>
            <input asp-for="BId" class="form-control" readonly>
        </div>

        <div class="form-group">
            <label asp-for="BName">FullName</label>
            <input asp-for="BName" class="form-control" readonly>
        </div>

        <div class="form-group">
            <label asp-for="BCNum">Contact Number</label>
            <input asp-for="BCNum" class="form-control" readonly>
        </div>

        <div class="form-group">
            <label asp-for="BDate">Date</label>
            <input asp-for="BDate" class="form-control" readonly />
        </div>


        <div class="form-group">
            <label asp-for="BPNum">Number of Persons</label>
            <input asp-for="BPNum" class="form-control" readonly>
        </div>

        <div class="form-group">
            <label asp-for="BMessage">Message</label>
            <input asp-for="BMessage" class="form-control" readonly></>
        </div>

        <button type="submit" class="btn btn-primary">Accept</button>
        <button type="button" onclick="history.back()" class="btn btn-secondary">Cancel</button>
    </form>

    <a href="#" class="scroll-top d-flex align-items-center justify-content-center">
        <i class="bi bi-arrow-up-short"></i>
    </a>

    <div id="preloader"></div>
    <script src="~/wwwroot/about.jpg"></script>
    <script src="~/vendor/aos/aos.js"></script>
    <script src="~/vendor/glightbox/js/glightbox.min.js"></script>
    <script src="~/vendor/purecounter/purecounter_vanilla.js"></script>
    <script src="~/vendor/swiper/swiper-bundle.min.js"></script>
    <script src="~/vendor/php-email-form/validate.js"></script>

    <!-- Template Main JS File -->
    <script src="~/js/site.js"></script>
</body>