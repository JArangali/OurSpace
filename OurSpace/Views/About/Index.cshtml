@{
    ViewData["Title"] = "About Us";
}
@model Bookings
<head>

    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">

    <title>OurSpace</title>

    <meta content="" name="description">
    <meta content="" name="keywords">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Open+Sans:ital,wght@0,300;0,400;0,500;0,600;0,700;1,300;1,400;1,600;1,700&family=Amatic+SC:ital,wght@0,300;0,400;0,500;0,600;0,700;1,300;1,400;1,500;1,600;1,700&family=Inter:ital,wght@0,300;0,400;0,500;0,600;0,700;1,300;1,400;1,500;1,600;1,700&display=swap"
          rel="stylesheet">
    <link href="~/img/favicon.png" rel="icon">
    <link href="~/img/apple-touch-icon.png" rel="apple-touch-icon">
    <link href="~/css/main.css" rel="stylesheet">
</head>
<body>
    <main id="main">

        <!-- ======= Hero Section ======= -->
        <section id="hero">
            <div class="container" style="padding-top: 60px; margin-bottom: 60px;">
                <div class="row justify-content-between gy-5">
                    <div class="col-lg-5 order-1 order-lg-2 text-center text-lg-start">
                        <img src="~/hero-img.png" class="img-fluid" alt="" data-aos="zoom-out" data-aos-delay="300">
                    </div>
                    <div class="col-lg-5 order-2 order-lg-1 d-flex flex-column justify-content-center align-items-center align-items-lg-start text-center text-lg-start">
                        <h2 data-aos="fade-up">For your Mental Health<br>Chill and Work</h2>
                        <p class="fst-italic" data-aos="fade-up">
                            Giving you the quality and ambient place for your needs. The aim of OurSpace is to give you the most
                        </p>
                        <ul data-aos="fade-up">
                            <li><i class="bi bi-check2-all"></i> Affordable</li>
                            <li><i class="bi bi-check2-all"></i> Perfect</li>
                            <li><i class="bi bi-check2-all"></i> Near</li>
                            <li><i class="bi bi-check2-all"></i> Amazing</li>
                            <li><i class="bi bi-check2-all"></i> Student Friendly</li>
                        </ul>
                        <p data-aos="fade-up">
                            Place to relax, study, work and help achieve your Goals and Dreams!
                        </p>
                        <div class="d-flex" data-aos="fade-up" data-aos-delay="200">
                        </div>
                    </div>
                </div>
            </div>
        </section><!-- End Hero Section -->
        </div>
        <!-- ======= Stats Counter Section ======= -->
        <section id="stats-counter" class="stats-counter">
            <div class="container" data-aos="zoom-out">

                <div class="row gy-4">

                    <div class="section-header">
                        <strong>
                            <h2 style="color: white;">6 Hubs</h2>
                            <p style="color: white;">and
                            <p> <span>More to</span>
                            <p style="color: white;"> Come!</p>
                        </strong>
                    </div>
                </div>

            </div>
        </section><!-- End Stats Counter Section -->
        <!-- ======= Chefs Section ======= -->
        <section id="chefs" class="chefs section-bg">
            <div class="container" data-aos="fade-up">

                <div class="section-header">
                    <h2>Astronauts</h2>
                    <p>Meet <span>Our</span> Team</p>
                </div>

                <div class="row gy-4">

                    <div class="col-lg-4 col-md-6 d-flex align-items-stretch" data-aos="fade-up" data-aos-delay="100">
                        <div class="chef-member">
                            <div class="member-img">
                                <img src="~/img/chefs/CruzMB.jpg" class="img-fluid" alt="">
                            </div>
                            <div class="member-info">
                                <h4>Mikhaela Cruz</h4>
                                <span>Developer</span>
                                <p>
                                    Hi, I'm Mika! I'm a passionate developer, always eager to learn and explore new technologies.
                                    I thrive on the challenge of coding and continuously refining my skills in the dynamic world of software development.
                                    We started OurSpace to offer solutions to finding dedicated spaces for productivity.
                                </p>
                            </div>
                        </div>
                    </div><!-- End Chefs Member -->

                    <div class="col-lg-4 col-md-6 d-flex align-items-stretch" data-aos="fade-up" data-aos-delay="200">
                        <div class="chef-member">
                            <div class="member-img">
                                <img src="~/img/chefs/Jas.jpg" class="img-fluid" alt="">
                            </div>
                            <div class="member-info">
                                <h4>Jasper Lindell Arangali</h4>
                                <span>Developer</span>
                                <p>
                                    Hey there, I'm Jasper Lindell Arangali, but you can just call me Jas. 
                                    I'm currently immersed in the world of IT as a student at the University of Santo Tomas.
                                    My goal is pretty straightforward—I want to leave a mark by creating websites and applications
                                    that elevate the way we experience technology. I'm passionate about leveraging my 
                                    skills to enhance the human experience through innovative digital solutions.
                                </p>
                            </div>
                        </div>
                    </div><!-- End Chefs Member -->

                    <div class="col-lg-4 col-md-6 d-flex align-items-stretch" data-aos="fade-up" data-aos-delay="300">
                        <div class="chef-member">
                            <div class="member-img">
                                <img src="~/img/chefs/lourds.jpeg" class="img-fluid" alt="">
                            </div>
                            <div class="member-info">
                                <h4>Lourdes Ashley Santos</h4>
                                <span>Developer</span>
                                <p>
                                    Hi, I’m Ashley! One of the developers of Our Space.
                                    We started this website to help fellow individuals to book study hubs for their productivity.
                                </p>
                            </div>
                        </div>
                    </div><!-- End Chefs Member -->
                </div>
                <center>
                    <div class="row gy-4" style="margin-top: 20px;">
                        <div class="col-lg-4 col-md-6 d-flex align-items-stretch" data-aos="fade-up" data-aos-delay="200">
                            <div class="chef-member">
                                <div class="member-img">
                                    <img src="~/img/chefs/SantosJP (1).jpg" class="img-fluid" alt="">
                                </div>
                                <div class="member-info">
                                    <h4>Jose Paolo Santos</h4>
                                    <span>Developer</span>
                                    <p>
                                        I'm Paolo! a passionate software developer with a love for clean code and problem-solving. 
                                        Dedicated to continuous learning and staying at the forefront of technology trends.
                                    </p>
                                </div>
                            </div>
                        </div><!-- End Chefs Member -->

                        <div class="col-lg-4 col-md-6 d-flex align-items-stretch" data-aos="fade-up" data-aos-delay="200">
                            <div class="chef-member">
                                <div class="member-img">
                                    <img src="~/img/chefs/MonteclaroE (1).JPG" class="img-fluid" alt="">
                                </div>
                                <div class="member-info">
                                    <h4>Elpidio Monteclaro</h4>
                                    <span>Developer</span>
                                    <p>
                                        Hello there, I am Elpidio Monteclaro but you can call me Ej for short.
                                        I am a learning web developer and a studen from University of Santo Tomas that
                                        is still in the process of acquiring new skills and expanding my knowledge.
                                        If you need any help, and its within my knowledge, dont hesitate to ask me.
                                    </p>
                                </div>
                            </div>
                        </div><!-- End Chefs Member -->

                    </div>
                </center>

            </div>
        </section><!-- End Chefs Section -->
        <!-- Start Footer -->
        </main>
        <footer id="footer" class="footer">
            <div class="footer">
                <div class="copyright">
                    &copy; Copyright <strong><span>OurSpace</span></strong>. All Rights Reserved
                </div>
                <div class="credits">
                    Designed by <a href="https://bootstrapmade.com/">Group 3 3ITF</a>
                </div>
            </div>
        </footer>

        <!-- End Footer -->
        <a href="#" class="scroll-top d-flex align-items-center justify-content-center">
            <i class="bi bi-arrow-up-short"></i>
        </a>

        <div id="preloader"></div>
        <script src="~/wwwroot/about.jpg"></script>
        <script src="~/vendor/aos/aos.js"></script>
        <script src="~/vendor/glightbox/js/glightbox.min.js"></script>
        <script src="~/vendor/purecounter/purecounter_vanilla.js"></script>
        <script src="~/vendor/swiper/swiper-bundle.min.js"></script>
        <script src="~/vendor/php-email-form/validate.js"></script>

        <!-- Vendor JS Files -->
        <script src="~/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
        <script src="~/vendor/aos/aos.js"></script>
        <script src="~/vendor/glightbox/js/glightbox.min.js"></script>
        <script src="~/vendor/purecounter/purecounter_vanilla.js"></script>
        <script src="~/vendor/swiper/swiper-bundle.min.js"></script>
        <script src="~/vendor/php-email-form/validate.js"></script>

        <!-- Template Main JS File -->
        <script src="~/js/main.js"></script>

</body>
