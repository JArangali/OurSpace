@model User


<head>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <title>OurSpace</title>
    <meta content="" name="description">
    <meta content="" name="keywords">
    <link href="~/img/favicon.png" rel="icon">
    <link href="~/apple-touch-icon.png" rel="apple-touch-icon">
    <link href="~/css/main.css" rel="stylesheet">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Open+Sans:ital,wght@0,300;0,400;0,500;0,600;0,700;1,300;1,400;1,600;1,700&family=Amatic+SC:ital,wght@0,300;0,400;0,500;0,600;0,700;1,300;1,400;1,500;1,600;1,700&family=Inter:ital,wght@0,300;0,400;0,500;0,600;0,700;1,300;1,400;1,500;1,600;1,700&display=swap"
          rel="stylesheet">
    <link href="~/vendor/bootstrap/css/bootstrap-grid.css" rel="stylesheet">
    <link href="~/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
    <link href="~/vendor/aos/aos.css" rel="stylesheet">
    <link href="~/vendor/glightbox/css/glightbox.min.css" rel="stylesheet">
    <link href="~/vendor/swiper/swiper-bundle.min.css" rel="stylesheet">

</head>
<body>
    <main id="main">
        <section id="hero">
            <form asp-action="Login" asp-controller="Account">

            <section>
                <div class="sign-up-container">
                    <div class="container">
                        <div asp-validation-summary="All" class="text-danger"></div>
                        <h1 style="font-family:Bahnschrift ; text-align:center">Log In</h1>


                        <!-- Email input -->
                        <div class="form-floating mb-4">
                            <input asp-for="UUsername" class="form-control" placeholder="" />
                            <label asp-for="UUsername">Username</label>
                        </div>

                        <!-- Password input -->
                        <div class="form-floating mb-4">
                            <input asp-for="UPassword" class="form-control" placeholder="" />
                            <label asp-for="UPassword">Password</label>
                        </div>



                        <div class="text-center">
                            <p>Don't have an account yet? <a asp-area="" asp-controller="Home" asp-action="SignUp"> Sign Up</a>! </p>
                            <button type="submit" class="sign-up-button">Sign In</button>
                        </div>


                    </div>

                </div>
            </section
        </form>
</section><!-- End Stats Counter Section -->

<link href="//maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
<script src="//maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"></script>
<script src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<header id="header" class="header fixed-top d-flex align-items-center">


    <div class="container d-flex align-items-center justify-content-between">
        <a asp-action="Index" class="logo d-flex align-items-center me-auto me-lg-0">
            <!-- Uncomment the line below if you also wish to use an image logo -->
            <!-- <img src="assets/img/logo.png" alt=""> -->
            <h1>OurSpace<span></span></h1>
        </a>
        <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Cancel</a>
    </div>
    <!-- Start Footer -->
    <footer id="footer" class="footer">
        <div class="footer">
            <div class="copyright">
                &copy; Copyright <strong><span>OurSpace</span></strong>. All Rights Reserved
            </div>
            <div class="credits">
                Designed by <a href="https://bootstrapmade.com/">Group 3 3ITF</a>
            </div>
        </div>
    </footer>

    <!-- End Footer -->
    <a href="#" class="scroll-top d-flex align-items-center justify-content-center">
        <i class="bi bi-arrow-up-short"></i>
    </a>

    <div id="preloader"></div>
    <script src="~/wwwroot/about.jpg"></script>
    <script src="~/vendor/aos/aos.js"></script>
    <script src="~/vendor/glightbox/js/glightbox.min.js"></script>
    <script src="~/vendor/purecounter/purecounter_vanilla.js"></script>
    <script src="~/vendor/swiper/swiper-bundle.min.js"></script>
    <script src="~/vendor/php-email-form/validate.js"></script>

    <!-- Template Main JS File -->
    <script src="~/js/site.js"></script>
</body>


