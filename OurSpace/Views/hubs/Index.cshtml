@{
    ViewData["Title"] = "Hubs";
}
<head>
    <link href="~/css/hub.css" rel="stylesheet" />
    <link href="~/img/favicon.png" rel="icon">
    <link href="~/img/apple-touch-icon.png" rel="apple-touch-icon">


    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Open+Sans:ital,wght@0,300;0,400;0,500;0,600;0,700;1,300;1,400;1,600;1,700&family=Amatic+SC:ital,wght@0,300;0,400;0,500;0,600;0,700;1,300;1,400;1,500;1,600;1,700&family=Inter:ital,wght@0,300;0,400;0,500;0,600;0,700;1,300;1,400;1,500;1,600;1,700&display=swap"
          rel="stylesheet">

</head>
<body>
    <main id="main">

        <!-- ======= Hero Section ======= -->
        <section id="hero">
            <div class="container">
                <div class="row justify-content-between gy-5">
                    <div class="col-lg-5 order-2 order-lg-1 d-flex flex-column justify-content-center align-items-center align-items-lg-start text-center text-lg-start">
                        <h2 data-aos="fade-up">For your Mental Health<br>Chill and Work</h2>
                        <p data-aos="fade-up" data-aos-delay="100">Select from our multiple BEST Spaces for your Mindspace</p>
                        <div class="d-flex" data-aos="fade-up" data-aos-delay="200">
                            <!-- Add your content here as needed -->
                        </div>
                    </div>
                    <div class="col-lg-5 order-1 order-lg-2 text-center text-lg-start">
                        <img src="~/hero-img.png" class="img-fluid" alt="" data-aos="zoom-out" data-aos-delay="300">
                    </div>
                </div>
            </div>
        </section>

        <!-- End Hero Section -->
        <!-- ======= HUBS CHOICES ======= -->
        <section id="hub_choice" class="hub_choice section-bg">
            <div class="container" data-aos="fade-up">

                <div class="row gy-4">

                    <div class="col-lg-4" data-aos="fade-up" data-aos-delay="100">
                        <div class="hub_choice_box">
                            <h3>Homeroom Co-Working & Study Lounge</h3>
                            <p>
                                Giving you the quality work place
                            </p>
                            <div class="text-center">
                                <a asp-action="homeroom" asp-controller="hubs" class="more-btn" asp-area="">BOOK NOW!<i class="bx bx-chevron-right"></i></a>
                            </div>
                        </div>
                    </div><!-- End CHOICE -->

                    <div class="col-lg-4" data-aos="fade-up" data-aos-delay="100">
                        <div class="hub_choice_box">
                            <h3>933 Creatives</h3>
                            <p>
                                Empowering Students and Freelancers to Thrive in a Dynamic
                                Co-working Environment
                            </p>
                            <div class="text-center">
                                <a asp-action="creatives" asp-controller="hubs" class="more-btn" asp-area="">BOOK NOW!<i class="bx bx-chevron-right"></i></a>
                            </div>
                        </div>
                    </div><!-- End CHOICE -->

                    <div class="col-lg-4" data-aos="fade-up" data-aos-delay="100">
                        <div class="hub_choice_box">
                            <h3>Space 1851 Coworking Hub</h3>
                            <p>
                                Your space for freedom, growth, and success.
                            </p>
                            <div class="text-center">
                                <a asp-action="space" asp-controller="hubs" class="more-btn" asp-area="">BOOK NOW!<i class="bx bx-chevron-right"></i></a>
                            </div>
                        </div>
                    </div><!-- End CHOICE -->

                    <div class="col-lg-4" data-aos="fade-up" data-aos-delay="100">
                        <div class="hub_choice_box">
                            <h3>Plug It - Coworking Space & Study Hub</h3>
                            <p>
                                Your new way of studying. Offers a great co-working environment, productive ambience and quality.
                            </p>
                            <div class="text-center">
                                <a asp-action="plugit" asp-controller="hubs" class="more-btn" asp-area="">BOOK NOW!<i class="bx bx-chevron-right"></i></a>
                            </div>
                        </div>
                    </div><!-- End CHOICE -->

                    <div class="col-lg-4" data-aos="fade-up" data-aos-delay="100">
                        <div class="hub_choice_box">
                            <h3>107 Co-working by Macao Imperial Tea</h3>
                            <p>
                                A Co-Working space by Macao Imperial Tea
                            </p>
                            <div class="text-center">
                                <a asp-action="macao" asp-controller="hubs" class="more-btn" asp-area="">BOOK NOW!<i class="bx bx-chevron-right"></i></a>
                            </div>
                        </div>
                    </div><!-- End CHOICE -->

                    <div class="col-lg-4" data-aos="fade-up" data-aos-delay="100">
                        <div class="hub_choice_box">
                            <h3>Stream Concept</h3>
                            <p>
                                Stream Concept Study Hub is a cafe themed, budget friendly coworking space.
                            </p>
                            <div class="text-center">
                                <a asp-action="stream" asp-controller="hubs" class="more-btn" asp-area="">BOOK NOW!<i class="bx bx-chevron-right"></i></a>


                            </div>
                        </div>
                    </div><!-- End CHOICE -->
                </div>
            </div>
            </div>
            </div>
        </section><!-- End HUBS CHOICES -->
        <!-- ======= Contact Section ======= -->
        <section id="contact" class="contact">
            <div class="container" data-aos="fade-up">

                <div class="section-header">
                    <h2>Google Maps</h2>
                    <p>Spaces <span>Here!</span></p>
                </div>

                <div class="mb-3">
                    <iframe style="border:0; width: 100%; height: 650px;"
                            src="https://www.google.com/maps/d/embed?mid=1oByBCQ4D4VkZhaP2L_HAzLTZNMAxI0E&ehbc=2E312F" frameborder="0"
                            allowfullscreen></iframe>
                </div><!-- End Google Maps -->

            </div>
        </section><!-- End Contact Section -->
    </main><!-- End #main -->
    <!-- Start Footer -->
    <footer id="footer" class="footer">
        <div class="footer">
            <div class="copyright">
                &copy; Copyright <strong><span>OurSpace</span></strong>. All Rights Reserved
            </div>
            <div class="credits">
                Designed by <a href="https://bootstrapmade.com/">Group 3 3ITF</a>
            </div>
        </div>
    </footer>

    <!-- End Footer -->

    <a href="#" class="scroll-top d-flex align-items-center justify-content-center">
        <i class="bi bi-arrow-up-short"></i>
    </a>

    <div id="preloader"></div>
    <script src="~/wwwroot/about.jpg"></script>
    <script src="~/vendor/aos/aos.js"></script>
    <script src="~/vendor/glightbox/js/glightbox.min.js"></script>
    <script src="~/vendor/purecounter/purecounter_vanilla.js"></script>
    <script src="~/vendor/swiper/swiper-bundle.min.js"></script>
    <script src="~/vendor/php-email-form/validate.js"></script>

    <!-- Vendor JS Files -->
    <script src="assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="assets/vendor/aos/aos.js"></script>
    <script src="assets/vendor/glightbox/js/glightbox.min.js"></script>
    <script src="assets/vendor/purecounter/purecounter_vanilla.js"></script>
    <script src="assets/vendor/swiper/swiper-bundle.min.js"></script>
    <script src="assets/vendor/php-email-form/validate.js"></script>

    <!-- Template Main JS File -->
    <script src="assets/js/main.js"></script>

</body>